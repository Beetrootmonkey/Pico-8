version 16
__lua__
--ld42 game jam entry
--by beetrootmonkey

--@import lib


p={}

function _init()
  p.x=20
  p.y=20
end

function _update()
  -- Player controls
  if btn(1) then p.x=p.x+1 end
  if btn(3) then p.y=p.y+1 end
  if btn(0) then p.x=p.x-1 end
  if btn(2) then p.y=p.y-1 end

  -- Lava update
  local x=rnd(16)
  local y=rnd(16)
  local t=mget(x, y)
  print('Hallo', 10, 10, 7)
  print(t, 0, 0, 7)
end

function _draw()
  cls()
  map()
  spr(69, p.x, p.y)
end

